{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "folding": {
        "markers": {
            "start": "\\b(do)\\b",
            "end": "\\b(end)\\b"
        }
    },
    "indentationRules": {
        "increaseIndentPattern": "\\b(do|if|while|for|function|fn|else|elsif)\\b.*$",
        "decreaseIndentPattern": "^\\s*(end|else|elsif)\\b"
    }
}